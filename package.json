{
  "name": "mimisalon-nextjs",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    ".",
    "packages/shared"
  ],
  "scripts": {
    "snapshot": "sudo -u postgres pg_dump -d petpet > ../$(date +%Y%m%d%H%M%S).sql",
    "dev": "next dev",
    "develop": "bun run --cwd packages/shared dev & bun dev",
    "build": "bun run --cwd packages/shared db:generate && next build",
    "start": "next start",
    "lint": "eslint --concurrency auto --cache .",
    "typecheck": "tsc --preserveWatchOutput --noEmit && bun run --cwd packages/shared typecheck",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:generate": "bun run --cwd packages/shared db:generate",
    "db:push": "bun run --cwd packages/shared db:push",
    "db:migrate": "bun run --cwd packages/shared db:migrate",
    "db:migrate:deploy": "bun run --cwd packages/shared db:migrate:deploy",
    "db:seed": "tsx packages/shared/prisma/seed.ts",
    "db:reset": "bun run --cwd packages/shared prisma migrate reset --force",
    "db:studio": "bun run --cwd packages/shared db:studio",
    "prepare": "husky",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docker:build": "docker build -t mimisalon:latest .",
    "docker:build:no-cache": "docker build --no-cache -t mimisalon:latest .",
    "docker:run": "docker run -d --name mimisalon -p 3000:3000 --env-file .env.production mimisalon:latest",
    "docker:logs": "docker logs -f mimisalon",
    "docker:stop": "docker stop mimisalon && docker rm mimisalon",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "docker:compose:build": "docker-compose build --no-cache"
  },
  "prisma": {
    "seed": "tsx packages/shared/prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{json,md,mdx,css,scss,ts,tsx,js,tsx}": [
      "prettier -w"
    ],
    "*.{ts,tsx}": [
      "bash -c 'tsc --noEmit'"
    ]
  },
  "dependencies": {
    "@google-cloud/storage": "^7.17.3",
    "@hookform/resolvers": "^5.2.2",
